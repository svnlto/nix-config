{
	"mcpServers": {
		"code-reasoning": {
			"command": "npx",
			"args": ["-y", "@mettamatt/code-reasoning"],
			"env": {}
		},
		"sequential-thinking": {
			"command": "npx",
			"args": ["-y", "@modelcontextprotocol/server-sequential-thinking"],
			"env": {}
		},
		"context7": {
			"command": "npx",
			"args": ["-y", "@upstash/context7-mcp@latest"],
			"env": {
				"DEFAULT_MINIMUM_TOKENS": "10000"
			}
		}
	}
}
